{
  "include": ["types", "src"],
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src",
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "paths": {
      "@types": ["../types"],
      "@DB": ["../ormconfig"],
      "@client/*": ["client/*"],
      "@models": ["shared/models/index"],
      "@admin/*": ["client/js/admin/*"],
      "@TransferDialog": ["client/js/admin/Transfers/TransferList/TransferDialog/index"],
      "@TransferDialogThunks": ["client/js/admin/Transfers/TransferList/TransferDialog/Thunks/index"],
      "@TransferActions/*": ["client/js/admin/Transfers/TransferList/TransferDialog/Actions/*"],
      "@login/*": ["client/js/login/*"],
      "@application/*": ["client/js/application/*"],
      "@dashboard/*": ["client/js/dashboard/*"],
      "@components/*": ["client/js/components/*"],
      "@containers/*": ["client/js/containers/*"],
      "@labels": ["client/js/labels"],
      "@brand/*": ["brand/*"],
      "@css/*": ["client/css/*"],
      "@store/*": ["client/js/store/*"],
      "@helpers": ["client/js/core/helpers"],
      "@server/*": ["server/*"],
      "@controllers": ["server/controllers/index"],
      "@sockets": ["server/sockets/index"],
      "@headless": ["server/controllers/headless/index"],
      "@email": ["shared/email/index"],
      "@jobs": ["server/jobs/index"],
      "@lib/*": ["server/lib/*"],
      "@middleware/*": ["server/middleware/*"],
      "@serverLabels": ["server/lib/labels"],
      "@log": ["server/lib/log"],
      "@api": ["shared/api/index"],
      "@validation": ["shared/validation"],
      "@entities": ["server/entities/index"],
      "@interfaces": ["shared/interfaces/index"],
      "@transformers": ["server/entities/Transformers"],
      "@repositories": ["server/repositories/index"],
      "@repositories/*": ["server/repositories/*"],
      "@numbers": ["shared/numbers"],
      "@util": ["shared/util"],
      "@USStates": ["shared/states"]
    },
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
        "dom",
        "es6"
    ],
    "jsx": "react",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true
  }
}